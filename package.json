{
  "name": "workbench",
  "version": "1.0.0",
  "description": "Browser-based tools for finding and classifying hieroglyphs in images",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/hieroglyphics-initiative/workbench.git"
  },
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --crossOrigin=use-credentials",
    "build-serve": "yarn build && superstatic ./dist --port 8080",
    "bundle-report": "webpack-bundle-analyzer ./dist/stats.json",
    "test": "ng test",
    "test-ci": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox",
    "lint": "npm run lint-scss && npm run lint-ts",
    "lint-scss": "nstylelint './src/**/*.scss'",
    "lint-ts": "ng lint",
    "e2e": "ng e2e",
    "extract": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/{en,ar}.json --clean --sort --format namespaced-json"
  },
  "dependencies": {
    "@angular/animations": "^8.2.10",
    "@angular/common": "^8.2.10",
    "@angular/compiler": "^8.2.10",
    "@angular/core": "^8.2.10",
    "@angular/forms": "^8.2.10",
    "@angular/platform-browser": "^8.2.10",
    "@angular/platform-browser-dynamic": "^8.2.10",
    "@angular/router": "^8.2.10",
    "@angular/service-worker": "8.2.10",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-pwa/local-storage": "^7.4.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "array-move": "^2.2.2",
    "blueimp-load-image": "^2.31.0",
    "buffer": "^5.6.0",
    "clone-deep": "^4.0.0",
    "core-js": "^2.5.6",
    "fabric": "^2.7.0",
    "hammerjs": "^2.0.8",
    "js-yaml": "^3.14.0",
    "jsfeat": "^0.0.8",
    "lodash": "^4.17.19",
    "mousetrap": "^1.6.3",
    "ngx-smart-modal": "^7.4.1",
    "rxjs": "^6.6.0",
    "sortablejs": "^1.9.0",
    "tooltip.js": "^1.3.2",
    "tslib": "^1.13.0",
    "uuid": "^3.2.1",
    "webfontloader": "^1.6.28",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.4",
    "@angular/cli": "^8.3.29",
    "@angular/compiler-cli": "^8.2.10",
    "@angular/language-service": "^8.2.10",
    "@types/fabric": "^2.6.12",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "^2.8.17",
    "@types/jasminewd2": "^2.0.6",
    "@types/mousetrap": "^1.6.2",
    "@types/node": "^11.15.20",
    "codelyzer": "^5.2.2",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^5.4.4",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.18.0",
    "superstatic": "^6.0.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.16.0",
    "typescript": "~3.5.3",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "peerDependencies": {}
}
